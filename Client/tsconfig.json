{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      // "@CampusLink/source": ["./src/*"],
      "@CampusLink/coreComponents":["./src/components"],
      "@CampusLink/common":["./src/common"],
      "@CampusLink/core":["./src/core"],
      "@CampusLink/auth":["./src/auth"],
      "@CampusLink/assets":["./src/assets"],
      "@CampusLink/pages":["./src/pages"],
      "@CampusLink/setting-module":["./src/libs/settings-module"],
      "@CampusLink/calendar-module":["./src/libs/calendar-module"],
      "@CampusLink/appComponents":["./src/libs/components"],
      "@CampusLink/hooks":["./src/libs/hooks"],
      "@CampusLink/config":["./src/libs/config"],
      "@CampusLink/helper":["./src/libs/helper"]
    },
    "allowJs": true,
    "checkJs": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
